{
  "name": "KorapayBookClub",
  "version": "0.1.0",
  "private": true,
  "licence": "MIT",
  "author": "Kelvin Omozokpia",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --noemit",
    "lint": "npm run check-types && next lint --cache",
    "format": "prettier --write \"**/*.+(ts|js|jsx|json|yml|yaml|css|md|vue)\" --cache",
    "precommit": "npm run format && npm run lint && git add -A",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.+(ts|tsx)": "eslint --fix --cache",
    "*.+(json|css|md)": "prettier --write --cache"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@iconify/icons-eva": "^1.2.6",
    "@iconify/react": "^4.1.1",
    "@mui/icons-material": "^5.14.0",
    "@mui/lab": "^5.0.0-alpha.136",
    "@mui/material": "^5.14.0",
    "@reduxjs/toolkit": "^1.9.6",
    "@svgr/webpack": "^8.1.0",
    "autosuggest-highlight": "^3.3.4",
    "next": "13.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.1.2",
    "swiper": "^10.3.0"
  },
  "devDependencies": {
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "5.1.6"
  }
}
